{
    "extends": ["prettier", "eslint:recommended", "plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "rules": {
      "prefer-const": "error"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  }